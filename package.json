{
  "name": "securelogic-engine",
  "version": "0.3.2",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npx tsc -p tsconfig.prod.json",
    "typecheck": "npx tsc -p tsconfig.ci.json",
    "lint": "eslint .",
    "test": "vitest run",
    "ci": "bash scripts/check-contract-version.sh && npm run typecheck && npm run build",
    "build:contracts": "cd packages/contracts && npm run build",
    "intake": "tsx src/intake/api/server.ts"
  },
  "devDependencies": {
    "@types/express": "~5.0.6",
    "@types/multer": "~2.0.0",
    "@types/node": "~25.0.3",
    "@types/pdfkit": "~0.17.4",
    "jscodeshift": "~17.3.0",
    "tsx": "~4.21.0",
    "typescript": "~5.9.3",
    "vitest": "~4.0.16"
  },
  "dependencies": {
    "ajv": "~8.17.1",
    "ajv-formats": "~3.0.1",
    "express": "~5.2.1",
    "multer": "~2.0.2",
    "pdfkit": "~0.17.2",
    "securelogic-contracts": "~0.1.2",
    "zod": "~4.2.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
