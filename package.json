{
  "name": "securelogic-engine",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc && npm run copy",
    "copy": "rsync -a frameworks dist/ --exclude '*.ts'",
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch engines --watch src --exec \"npm run build && npm start\"",
    "api": "ts-node src/server/index.ts"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fast-deep-equal": "^3.1.3",
    "pdfkit": "^0.15.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
