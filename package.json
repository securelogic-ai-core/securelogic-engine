{
  "name": "securelogic-engine",
  "version": "0.1.6",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run build:contracts && npx tsc -p tsconfig.prod.json",
    "typecheck": "npx tsc -p tsconfig.ci.json",
    "lint": "eslint .",
    "test": "vitest run",
    "ci": "npm run typecheck && npm run build && npm run test",
    "build:contracts": "cd packages/contracts && npm run build"
  },
  "devDependencies": {
    "@types/node": "~25.0.3",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@securelogic/contracts": "file:packages/contracts"
  }
}
